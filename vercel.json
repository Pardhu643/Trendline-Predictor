{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/output.css",
      "dest": "/dist/output.css"
    },
    {
      "src": "/(.*)", 
      "dest": "/index.html"
    }
  ]
}